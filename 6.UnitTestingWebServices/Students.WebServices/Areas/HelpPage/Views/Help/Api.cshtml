@using Students.WebServices.Areas.HelpPage.Models

@model HelpPageApiModel

@{
    var description = this.Model.ApiDescription;
    this.ViewBag.Title = string.Format("{0} {1}", description.HttpMethod.Method, description.RelativePath);
}

<div id="body">
    <section class="featured">
        <div class="content-wrapper">
            <p>
                @this.Html.ActionLink("Help Page Home", "Index")
            </p>
        </div>
    </section>
    <section class="content-wrapper main-content clear-fix">
        @this.Html.DisplayFor(m => this.Model)
    </section>
</div>

@section Scripts {
    <link type="text/css" href="~/Areas/HelpPage/HelpPage.css" rel="stylesheet" />
}